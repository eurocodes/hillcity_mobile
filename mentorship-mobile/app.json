{
  "expo": {
    "name": "mentorship-mobile",
    "slug": "mentorship-mobile",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "usesIcloudStorage": true
    },
    "android": {
      "package": "com.hillcity.mentorship_mobile",
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true
    },
    "description": "Hillcity Mentorship Mobile ",
    "githubUrl": "https://github.com/eurocodes/hillcity_mobile"
  }
}
